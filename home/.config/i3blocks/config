[network]
command=~/.config/i3blocks/network/network
interval=10

[volume]
command=~/.config/i3blocks/volume/volume
interval=once
signal=10

[cpu_temp]
command=~/.config/i3blocks/cpu_temp/cpu_temp
interval=10

[brightness]
command=~/.config/i3blocks/brightness/brightness
interval=once
signal=11

[date]
command=date "+%d.%m.%Y"
interval=60

[time]
command=date "+%H:%M"
interval=5
